#!/bin/sh

OBJ="../libs/ficheros_basico.o ../libs/bloques.o ../libs/ficheros.o ../libs/directorios.o ../libs/semaforos.o"

rm -f infosb
rm -f infomb
rm -f RWBits
rm -f RWBlocs
rm -f RWInodes
rm -f traduir
rm -f cercar
rm -f truncar

gcc -Wall -O1 -ggdb infosb.c ${OBJ} -o infosb
gcc -Wall -O1 -ggdb infomb.c ${OBJ} -o infomb
gcc -Wall -O1 -ggdb RWBits.c ${OBJ} -o RWBits
gcc -Wall -O1 -ggdb RWBlocs.c ${OBJ} -o RWBlocs
gcc -Wall -O1 -ggdb RWInodes.c ${OBJ} -o RWInodes
gcc -Wall -O1 -ggdb traduir.c ${OBJ} -o traduir
gcc -Wall -O1 -ggdb cercar.c ${OBJ} -o cercar
gcc -Wall -O1 -ggdb truncar.c ${OBJ} -o truncar
echo "OK TEST"
